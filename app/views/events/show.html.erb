   <div class="row">
     <div class="col-md-6 ">
       <h2>Attendees for event:</h2>
       <h4>" <%= @event.name %> "</h4><br>
       <ul>
          <% @event.attendees.each do |attendee| %>
          <li><b><p><%= attendee.name %></p></b></li>
           <% end %>
       </ul>
      </div>
      <%= render partial: 'invited_users', locals: {event: @event,users: @users} %>
   </div>
