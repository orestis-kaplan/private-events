<div class="row">
  <div class="text-center">
    <h1>Your Events</h1>
  </div>
  <div class="col-md-6 text-right">
    <h2> Previous </h2>
      <% @previous_events.each do |eofa| %>
        <p><b><%= link_to eofa.name ,event_path(eofa)%></b></p><br>
      <% end %>
  </div>
  <div class="col-md-6 ">
    <h2> Upcoming </h2>
      <% @upcoming_events.each do |eofa| %>
        <p><b><%= link_to eofa.name ,event_path(eofa)%></b></p><br>
      <% end %>
  </div>
</div>
